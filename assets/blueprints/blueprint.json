			},
			"options": {
				"activate": true
			}
		},
		{
			"step": "importFile",
			"file": {
				"resource": "url",
				"url": "https://raw.githubusercontent.com/stellarwp/kadence-blocks/master/.wordpress-org/blueprints/example-page-demo.xml"
			}
		}
